<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% foundAW.forEach(function(AW) { %>
        <div class="an-artwork">
            <div class="artworkname">
                <%= AW.artworkName %>
            </div>
            <div class="artworkCategories">
                <%= AW.categories %>
            </div>
            <form action="/detail/<%= AW._id %>/save" method="post">
                <button type="submit">Save</button>
            </form>
        </div>
    <% }) %>
</body>
</html>