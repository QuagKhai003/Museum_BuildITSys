<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% allAW.forEach(function(AW) { %>
        <div class="an-artwork">
            <a class="artworkname" href="/detail/<%= AW._id %>">
                <%= AW.artworkName %>
            </a>
            <div class="artworkCategories">
                <%= AW.categories %>
            </div>
        </div>
    <% }) %>
</body>
</html>